doctype html
html
    head
        title Shopping history
        link(rel='stylesheet', href='/stylesheets/style.css')
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css' integrity='sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T' crossorigin='anonymous')
    body
        nav.navbar.navbar-dark.fixed-top.bg-dark.flex-md-nowrap.p-0.shadow
            a.navbar-brand.col-sm-3.col-md-2.mr-0(href='#') Shopping history
            input.form-control.form-control-dark.w-100(type='text', placeholder='Search', aria-label='Search')
            ul.navbar-nav.px-3
                li.nav-item.text-nowrap
                    a.nav-link(href='#') Enter
        .container-fluid
            .row
                nav.col-md-2.d-none.d-md-block.bg-light.sidebar
                    .sidebar-sticky
                        ul.nav.flex-column
                            li.nav-item
                                a.nav-link.active(href='/')
                                    span(data-feather='home')
                                    |  Dashboard
                                    span.sr-only (current)
                            li.nav-item
                                a.nav-link(href='/list/user')
                                    span(data-feather='file')
                                    |  Users
                            li.nav-item
                                a.nav-link(href='/list/item')
                                    span(data-feather='shopping-cart')
                                    |  Items
                            li.nav-item
                                a.nav-link(href='/list/order')
                                    span(data-feather='users')
                                    |  Orders
                        h6.sidebar-heading.d-flex.justify-content-between.align-items-center.px-3.mt-4.mb-1.text-muted
                            span DATA MANAGEMENT
                            a.d-flex.align-items-center.text-muted(href='#')
                                span(data-feather='plus-circle')
                        ul.nav.flex-column.mb-2
                            li.nav-item
                                a.nav-link(href='/management/user')
                                    span(data-feather='file-text')
                                    |  User data
                            li.nav-item
                                a.nav-link(href='/management/item')
                                    span(data-feather='file-text')
                                    |  Item data
                            li.nav-item
                                a.nav-link(href='/management/order')
                                    span(data-feather='file-text')
                                    |  Order data
                main.col-md-9.ml-sm-auto.col-lg-10.px-4(role='main')
                    block content
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js' integrity='sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1' crossorigin='anonymous')
        script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js' integrity='sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM' crossorigin='anonymous')
        script(src='/javascripts/event.js')